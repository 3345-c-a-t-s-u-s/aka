getfenv().TableToString = function(t,orgin:Instance)
	local str = stringify(t)
	local upp = [[-- Path: ]]..orgin:GetFullName()..'; \n'..str
	SideData.Value = upp
	SideData.Path = orgin:GetFullName()
	for i,v in ipairs(MouseTarget:GetChildren()) do
		if v:IsA('TextLabel') then
			v:Destroy()
		end
	end
	
	for line in upp:gmatch("[^\n]+") do
		local Line = Instance.new("TextLabel")
		Line.Name = "Line"
		Line.Parent = MouseTarget
		Line.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Line.BackgroundTransparency = 1.000
		Line.BorderColor3 = Color3.fromRGB(255, 255, 255)
		Line.BorderSizePixel = 0
		Line.Position = UDim2.new(0, 0, -1.50927676e-07, 0)
		Line.Size = UDim2.new(3.27953267, 0, 0, 15)
		Line.ZIndex = 8
		Line.Font = Enum.Font.SourceSans
		Line.TextColor3 = Color3.fromRGB(255, 255, 255)
		Line.TextScaled = true
		Line.TextSize = 14.000
		Line.TextWrapped = true
		Line.TextXAlignment = Enum.TextXAlignment.Left
		Line.Text = line
	end
	
	MouseTarget.CanvasSize = UDim2.new(0,UIListLayout.AbsoluteContentSize.X,0,UIListLayout.AbsoluteContentSize.Y)
end

