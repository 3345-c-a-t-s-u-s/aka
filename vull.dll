getfenv().VisitObject = function(instance:Instance)
	if not instance then
		return {}
	end
	
	local dex = {}
	for i,v in ipairs(instance:GetChildren()) do
		if #v:GetChildren() == 0 then
			dex[tostring(v)] = {
				ClassName = v.ClassName,
			}
		else
			dex[tostring(v)] = {
				ClassName = v.ClassName,
				Children = VisitObject(v)
			}
		end
	end
	
	return dex
end

getfenv().stringify = function(v, spaces, usesemicolon, depth)
	if type(v) ~= 'table' then
		if typeof(v) == 'string' then
			return '"'..tostring(v)..'"'
		else
			return tostring(v)
		end
	elseif not next(v) then
		return '{}'
	end

	spaces = spaces or 4
	depth = depth or 1

	local space = (" "):rep(depth * spaces)
	local sep = usesemicolon and ";" or ","
	local s = "{"

	for k, x in next, v do
		s = s..("\n%s[%s] = %s%s"):format(space,type(k)=='number'and tostring(k)or('"%s"'):format(tostring(k)), stringify(x, spaces, usesemicolon, depth+1), sep)
	end

	return ("%s\n%s}"):format(s:sub(1,-2), space:sub(1, -spaces-1))
end

